  m_Script: {fileID: 11500000, guid: 280198b86e48cc044b5f3d30aae76961, type: 3}
  m_Name: "(\uC2E4\uAC10\uD615Manual)HintPopupInfo"
  m_EditorClassIdentifier: 
  hintData:
  - title: "\uC218\uB3D9\uAE30\uB3D9\uBC29\uC2DD"
    text: "\uAC00\uC2A4\uACC4\uC18C\uD654\uC124\uBE44 \uC218\uB3D9\uAE30\uB3D9\uBC29\uC2DD(\uC218\uB3D9\uC870\uC791\uD568
      \uC791\uB3D9) \uC791\uB3D9\uC21C\uC11C\uC785\uB2C8\uB2E4."
    audioClip: {fileID: 8300000, guid: 510f07d556c54624ba454b149038180d, type: 3}
  - title: STEP 1
    text: "\uD654\uC7AC\uBC1C\uC0DD"
    audioClip: {fileID: 8300000, guid: da61dfc2dcbaa5d44a73936559478ef5, type: 3}
  - title: STEP 2
    text: "\uD654\uC7AC\uBC1C\uC0DD \uD655\uC778"
    audioClip: {fileID: 8300000, guid: ad8398aa6905f974db474ed2fd398b54, type: 3}
  - title: STEP 3
    text: "\uC218\uB3D9\uC870\uC791\uD568 \uAE30\uB3D9\uC2A4\uC704\uCE58 \uB204\uB984"
    audioClip: {fileID: 8300000, guid: 8357f9c09eb3ab94c844cfef6db83907, type: 3}
  - title: STEP 4
    text: "\uC81C\uC5B4\uBC18 \uD654\uC7AC\uD45C\uC2DC\uB4F1 \uC810\uB4F1, \uC218\uB3D9\uC870\uC791\uD568
      \uAE30\uB3D9\uD45C\uC2DC\uB4F1 \uC810\uB4F1 \uBC0F \uC57D\uC81C\uBC29\uCD9C
      \uC9C0\uC5F0\uC2DC\uAC04 \uC791\uB3D9"
    audioClip: {fileID: 8300000, guid: 836f0fec8cb79474fac5c96c9b94fca4, type: 3}
  - title: STEP 5
    text: "\uC194\uB808\uB178\uC774\uB4DC\uBC38\uBE0C \uACA9\uBC1C\uB85C \uAE30\uB3D9\uC6A9\uAC00\uC2A4\uC6A9\uAE30
      \uAC1C\uBC29"
    audioClip: {fileID: 8300000, guid: 7b916cc0f67f67048b49138c2255101a, type: 3}
  - title: STEP 6
    text: "\uAC10\uC2DC\uC81C\uC5B4\uBC18 \uC194\uB808\uB178\uC774\uB4DC \uBC38\uBE0C
      \uAE30\uB3D9 \uD45C\uC2DC\uB4F1 \uC810\uB4F1"
    audioClip: {fileID: 8300000, guid: a03b7909f160db341b1cead0be8f24d2, type: 3}
  - title: STEP 7
    text: "\uC120\uD0DD\uBC38\uBE0C \uAC1C\uBC29"
    audioClip: {fileID: 8300000, guid: e1b7da53e3e7a124596e2179783240e9, type: 3}
  - title: STEP 8
    text: "\uAE30\uB3D9\uC6A9\uAC00\uC2A4 \uC120\uD0DD\uBC38\uBE0C\uC640 \uC5F0\uACB0\uB41C
      \uB3D9\uAD00\uC744 \uD1B5\uD574 \uC800\uC7A5\uC6A9\uAE30\uB85C \uC774\uB3D9"
    audioClip: {fileID: 8300000, guid: 3e10f31ed6f77f14bbf6a00e1e12f23c, type: 3}
  - title: STEP 9
    text: "\uC800\uC7A5\uC6A9\uAE30 \uAC1C\uBC29"
    audioClip: {fileID: 8300000, guid: f25f3ca381694244490caea5bacc174b, type: 3}
  - title: STEP 10
    text: "\uC9D1\uD569\uAD00\uC744 \uD1B5\uD574 \uC18C\uD654\uAC00\uC2A4 \uC774\uB3D9"
    audioClip: {fileID: 8300000, guid: d46c426312f91684ba706ce3e3883b64, type: 3}
  - title: STEP 11
    text: "\uC555\uB825\uC2A4\uC704\uCE58 \uC791\uB3D9"
    audioClip: {fileID: 8300000, guid: 7d315bb020af42542b47beb57694864e, type: 3}
  - title: STEP 12
    text: "\uAC10\uC2DC\uC81C\uC5B4\uBC18 \uBC29\uCD9C \uD655\uC778 \uD45C\uC2DC\uB4F1
      \uC810\uB4F1"
    audioClip: {fileID: 8300000, guid: c6567cb48420f7a468a8df1b8dc07c37, type: 3}
  - title: STEP 13
    text: "\uBC29\uD638\uAD6C\uC5ED \uCD9C\uC785\uBB38 \uC0C1\uB2E8\uC758 \uBC29\uCD9C\uD45C\uC2DC\uB4F1
      \uBC0F \uC218\uB3D9\uC870\uC791\uD568 \uBC29\uCD9C\uB4F1 \uC810\uB4F1"
    audioClip: {fileID: 8300000, guid: de9cefe8ef9e75146b3be351e11e3c28, type: 3}
  - title: STEP 14
    text: "\uC18C\uD654\uAC00\uC2A4 \uBC29\uD638\uAD6C\uC5ED\uC73C\uB85C \uC774\uB3D9"
    audioClip: {fileID: 8300000, guid: 968b4345304e3d24fa15b8791765ac6d, type: 3}
  - title: STEP 15
    text: "\uD654\uC7AC \uC9C4\uC555"
    audioClip: {fileID: 8300000, guid: a9da42157fba86845b29c2d8eb2a2526, type: 3}
  - title: "\uC218\uB3D9\uAE30\uB3D9\uBC29\uC2DD"
    text: "\uC2E4\uC2B5\uC885\uB8CC"
    audioClip: {fileID: 8300000, guid: 7583da28e32413e49b599ed522260483, type: 3}
